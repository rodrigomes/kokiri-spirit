
<%= gmaps4rails(@json) %>

<h1>Listing trees</h1>

<table>
  <tr>
    <th><%= link_to 'Nickname', { controller: "trees", :action => :index, filter: 'nickname' } %></th>
    <th>Content</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th><%= link_to 'Created_at', { controller: "trees", :action => :index, filter: 'created_at' } %></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @trees.each do |tree| %>
  <tr>
    <td><%= tree.nickname %></td>
    <td><%= tree.content %></td>
    <td><%= tree.latitude %></td>
    <td><%= tree.longitude %></td>
    <td><%= tree.created_at %></td>
    <td><%= link_to 'Show', tree %></td>
    <td><%= link_to 'Edit', edit_tree_path(tree) %></td>
    <td><%= link_to 'Destroy', tree, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Tree', new_tree_path %>

